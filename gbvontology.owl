  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/ontology/gbv#dbkey">
    <rdfs:comment xml:lang="en">database key, used as prefix in unAPI and at other APIs within the GBV</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/ontology/gbv#dbkey">
    <skos:scopeNote xml:lang="en">must comply to the regular expression ^[a-z][a-z0-9-]*[a-z0-9]$</skos:scopeNote>
  </rdf:Description>
